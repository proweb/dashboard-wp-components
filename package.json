{
	"name": "wp-components-dashboard",
	"version": "0.1.0",
	"description": "WordPress Gutenberg components in wp-admin",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"server": "npx @wp-playground/cli server --auto-mount --login",
		"watch": "wp-scripts start --webpack-copy-php",
		"build": "wp-scripts build --webpack-copy-php",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "concurrently \"npm run server\" \"npm run watch\""
	},
	"devDependencies": {
		"@wordpress/scripts": "^31.1.0",
		"concurrently": "^9.2.1"
	},
	"dependencies": {
		"@wordpress/components": "^30.9.0",
		"@wordpress/icons": "^11.3.0"
	}
}
